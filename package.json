  },
  "homepage": "https://github.com/luigicr/accessibility-tests-boilerplate#readme",
  "devDependencies": {
    "bootstrap-sass": "^3.3.7",
    "express": "^4.15.3",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.0",
    "gulp-sass": "^3.1.0",
    "npm-run-all": "^4.0.2"
  }
