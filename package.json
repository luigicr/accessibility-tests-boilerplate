{
  "name": "accessibility-tests-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for accessibility tests from real world examples",
  "main": "index.js",
  "scripts": {
    "prestart": "npm-run-all --sequential gulp hellomessage",
    "start": "npm-run-all open:src",
    "hellomessage" : "node server/startMessage.js",
